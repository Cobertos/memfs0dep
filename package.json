{
  "name": "memfs0dep",
  "version": "4.36.3",
  "description": "In-memory file-system with Node's fs API. No deps. Forked from streamich/memfs",
  "keywords": [
    "fs",
    "filesystem",
    "fs.js",
    "memory-fs",
    "memfs",
    "file",
    "file system",
    "mount",
    "memory",
    "in-memory",
    "virtual",
    "test",
    "testing",
    "mock"
  ],
  "homepage": "https://github.com/Cobertos/memfs0dep",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cobertos/memfs0dep.git"
  },
  "license": "Apache-2.0",
  "author": [
    {
      "name": "streamich",
      "url": "https://github.com/streamich"
    },
    {
      "name": "cobertos",
      "url": "https://github.com/cobertos"
    }
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p . && rimraf --glob lib/__tests__ 'lib/**/__tests__'",
    "build:webfs": "NODE_ENV=production webpack --config ./src/webfs/webpack.config.js",
    "test": "jest --maxWorkers 2",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p .",
    "watch": "watch \"npm run build\" ./src",
    "coverage": "yarn test --collectCoverage",
    "typedoc": "typedoc --tsconfig tsconfig.json",
    "build:pages": "npx rimraf@5.0.5 gh-pages && mkdir -p gh-pages && cp -r typedocs/* gh-pages && cp -r coverage gh-pages/coverage"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "testRegex": ".*/__tests__/.*\\.(test|spec)\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@types/node": "^24.3.0",
    "jest": "^30.0.5",
    "semantic-release": "^24.2.7",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "@types/jest": "^30.0.0",
    "ts-jest": "^29.4.1"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "dependencies": {
  }
}
